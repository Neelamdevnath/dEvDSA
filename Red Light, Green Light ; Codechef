Problem link - https://www.codechef.com/problems/DOLL

code -
#include <iostream>
using namespace std;
int main() {
    int t ;
    cin >> t ;
    for (int i = 0 ; i <t ; i++){
        int maxh ;
        int numofinputs ;
        cin >> numofinputs;
        cin >> maxh ;
        int arr[numofinputs] ;
        int count = 0;
        for (int  jforindex = 0 ; jforindex < numofinputs ; jforindex++){
            
            
            cin >> arr[jforindex]; 
            if (arr[jforindex] > maxh ){
                count++;
            }
                
           }
           cout << count << endl ;
           
            }
    return 0;
}

